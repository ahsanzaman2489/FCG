{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-props-no-spreading": ["error", {"custom": "ignore"}],
    "prettier/prettier": [
      "error", {
        "semi": true
      }
    ],
    "import/prefer-default-export": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}